{
  "compilerOptions": {
    "types": ["react", "react-router"],
    "target": "ES6",
    "module": "es2015",
    "esModuleInterop": true,
    "jsx": "react",
    "removeComments": true,
    "strict": false,
    "outDir": "./build",
    "rootDir": "./",
    "moduleResolution": "node"
  },
  "exclude": ["assets", "build", "dist", "node_modules"]
}
